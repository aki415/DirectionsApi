package com.example.mapstemplate

data class DirectionsApiResponse(
    val routes: List<Route>
)
data class Route(
    val legs: List<Leg>
)
data class Leg(
    val steps: List<Step>
)
data class Step(
    val start_location: LatLng,
    val end_location: LatLng,
    val polyline: Polyline
)
data class LatLng(
    val lat: Double,
    val lng: Double
)
data class Polyline(
    val points: String
)
